{
	"scripts": {
		"test": "node scripts/test.js",
		"lint": "eslint . --ext .ts",
		"dev": "npx tsc -p . --watch",
		"build": "npx run-s build:*",
		"build:tsc": "npx tsc -p .",
		"build:extension": "npm run build-extension",
		"build:script": "node scripts/build.js",
		"send-build": "npm run build && node scripts/send.js",
		"cstart": "npx tsc -p . && node dist/main.js",
		"start": "node scripts/manage.js",
		"start-once": "node dist/main.js",
		"upload-all": "node scripts/upload-all.js",
		"build-extension": "npx run-s build-extension:*",
		"build-extension:tsc": "npx tsc -p .",
		"build-extension:previous": "node scripts/webpack-prev",
		"build-extension:background": "npx webpack --env BG=1",
		"build-extension:content": "npx webpack --env BG=0",
		"build-extension:after": "node scripts/webpack-after"
	},
	"dependencies": {
		"needle": "^3.0.0",
		"nightmare": "^2.8.0",
		"node-html-parser": "^4.1.2",
