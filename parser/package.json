		"request": "^2.88.2",
		"socket.io-client": "^4.1.3"
	},
	"devDependencies": {
		"@babel/core": "^7.15.5",
		"@babel/preset-env": "^7.15.6",
		"babel-loader": "^8.2.2",
		"@types/chrome": "0.0.157",
		"@types/needle": "^2.5.2",
		"@types/nightmare": "^2.10.5",
		"@types/website-scraper": "^1.2.6",
		"@typescript-eslint/eslint-plugin": "^4.31.0",
		"@typescript-eslint/parser": "^4.31.0",
		"adm-zip": "^0.5.5",
		"eslint": "^7.32.0",
		"typescript": "^4.3.5",
		"webpack": "^5.53.0",
		"webpack-cli": "^4.8.0",
		"website-scraper": "^5.0.0"
	}
}
