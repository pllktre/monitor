{
	"manifest_version": 2,
	"name": "Parser Extension",
	"version": "0.5.0",
	"permissions": ["storage", "https://*/*"],
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [{
		"js": ["content.js"],
		"matches": ["*://*/*"]
	}],
	"browser_action": {
		"default_popup": "popup.html"
	}
}